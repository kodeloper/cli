{
  "name": "kodeloper-cli",
  "version": "0.1.1",
  "description": "Kodeloper CLI helper tool",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build": "npx babel src --out-dir build --presets=env",
    "upload": "node -r dotenv/config ./build/index.js -- jira",
    "kodeloper": "npm run build && npm run upload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodeloper/cli.git"
  },
  "keywords": [
    "cli",
    "node",
    "kodeloper",
    "js",
    "development",
    "tools"
  ],
  "author": "kodeloper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kodeloper/cli/issues"
  },
  "homepage": "https://github.com/kodeloper/cli#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bitbucket-api-v2": "^0.6.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "dotenv": "^6.0.0",
    "flat-cache": "^1.3.0",
    "fs-extra": "^6.0.1",
    "ftp": "^0.3.10",
    "inquirer": "^6.0.0",
    "jira-client": "^6.4.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0"
  },
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8.10"
        }
      }
    ]
  ]
}
